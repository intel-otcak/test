 		break;
 	case OV2685_RES_2M:
 		ret = ov2685_write_reg_array(client, ov2685_2M_init);
@@ -1107,8 +1130,8 @@ static int ov2685_s_mbus_fmt(struct v4l2_subdev *sd,
